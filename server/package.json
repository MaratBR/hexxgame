{
  "name": "server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-ts": "yarn tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "build": "npm run lint && npm run build-ts && npm run copy-static-assets",
    "start": "npm run serve",
    "watch-server": "yarn run nodemon --watch server -e ts,tsx --exec ts-node server.ts",
    "dev": "yarn run watch-server"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.4.1",
    "bcrypt": "^5.0.0",
    "bull": "^3.18.1",
    "common": "1.0.0",
    "dotenv": "^8.2.0",
    "koa": "^2.13.0",
    "koa-cookie": "^1.0.0",
    "koa-cors": "^0.0.16",
    "koa-logger": "^3.2.1",
    "koa-passport": "^4.1.3",
    "koa-redis": "^4.0.1",
    "moment": "^2.29.1",
    "mongoose": "^5.10.13",
    "nanoid": "^3.1.16",
    "routing-controllers": "^0.9.0-alpha.6",
    "socket.io": "^3.0.0",
    "socket.io-redis": "^5.4.0",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/bull": "^3",
    "@types/koa": "^2",
    "@types/koa-cors": "^0",
    "@types/koa-logger": "^3",
    "@types/koa-passport": "^4",
    "@types/koa-redis": "^4",
    "@types/mongoose": "^5",
    "nodemon": "^2.0.6",
    "typescript": "^4.0.5"
  }
}
